<?xml version="1.0" encoding="UTF-8"?>

<report>
    <name>states</name>
	<reportDescription>
    	<name>states</name>
    	<displayName>states</displayName>
    </reportDescription>    
    <cells>
        <cell>
            <name>CELL1</name>
            <valueType>LONG</valueType>
            <cellState>
                <types>
                    <type.editable/>
                </types>
            </cellState>
        </cell>
        <cell>
            <name>CELL2</name>
            <valueType>LONG</valueType>
            <cellState>
                <types>
                    <type.editable/>
                </types>
            </cellState>
        </cell>
        <cell>
            <name>CELL3</name>
            <valueType>LONG</valueType>
            <cellState>
                <types>
                    <type.editable/>
                    <type.formula 
                	activeType="true">
                        <formula>
                        ${CELL1}+${CELL2}
                        </formula>
                    </type.formula>
                </types>
            </cellState>
        </cell>
        <cell>
            <name>CELL5</name>
            <valueType>LONG</valueType>
            <cellState>
                <types>
                    <type.accounts 
		      activeType="true">
                        <accounts>
                            <account 
			      accFrom="1000" 
			      accTo="ZZZZZZ" 
			      sign="+" 
			      type="debit"/>
                            <account 
			      accFrom="1000" 
			      accTo="AAAA" 
			      sign="-" 
			      type="debit"/>
                        </accounts>
                    </type.accounts>
                    <type.editable/>
                </types>
            </cellState>
        </cell>
    </cells>
</report>
